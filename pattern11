#include <stdio.h>

int main()
{
    int i,j,k,m=0,n;
    
   for(i=1;i<7;i++)
   {
      if((i==1)||(i==3)||(i==5)||(i==7))
        {
            m=0;
             for(k=0;k<i;k++)
                  {
                      m=m+i;
                       printf("%d ",m);
                          
                   }
        }
        else if((i==2)||(i==4)||(i==6))
            {
                n=0;
                 for(k=0;k<i;k++)
                  { 
                      n=n+i;
                       printf("%d ",n);
                          
                   }
                
                
            }
        printf("\n");   
   }
   
        
    return 0;
}
