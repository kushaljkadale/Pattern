#include <stdio.h>

int main()
{
    int i,j,k,m,n;
    
   for(i=0;i<5;i++)
   {
        m=1;
       for(j=0;j<5-i;j++)
       printf("  ");
       
       for(k=0;k<=i*2;k++)
       {
        printf("%d ",m);
        m++;
       }
        printf("\n");   
   }
   
   for(i=0;i<5;i++)
   {
       n=1;
       for(j=0;j<=i+1;j++)
       printf("  ");
       
       for(k=7;k>i*2;k--)
       {
        printf("%d ",n);
        n++;
       }
        printf("\n"); 
   }
        
    return 0;
}
