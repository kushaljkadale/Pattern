#include <stdio.h>
int main()
{
    int i,j,k,m=1,n,x=1,y=8,c=0;
    
   for(i=1;i<=5;i++)
   {
    
       for(j=0;j<10;j++)
      {
          
          if((j>=x) && (j<=y))
          {
              printf("  ");
          }
          else
          {
              printf("%d ",m);
           // c++;
           }
          
      }
   m=1;
      x++;
      y--;
       printf("\n");
   }
   printf("count=%d\n",c);
    return 0;

}
