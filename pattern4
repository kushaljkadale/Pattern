
#include <stdio.h>

int main()
{ 
    int i,j,k;
    
    for(i=0;i<5;i++)
    {
        for(k=0;k<=i;k++)
        printf(" ");
        
        for(j=0;j<9-i*2;j++)
        printf("*");
        
        printf("\n");
    }
   
    for(i=0;i<5;i++)
    {
        for(j=0;j<5-i;j++)
        printf(" ");
        
        for(k=0;k<=i*2;k++)
        printf("*");
        
        printf("\n");
    }
    
    return 0;
}
