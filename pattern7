#include <stdio.h>

int main()
{
    int i,j,k,m,n;
    
    
    for(i=1;i<5;i++)
    {
        if((i==1)||(i==3)||(i==5)||(i==7))
        {
            m=1;
            for(j=0;j<i;j++)
            {
                printf("%d",m);
                m=m+2;
            }
        }
        
        else if((i==2)||(i==4)||(i==6))
        {
            n=2;
            
            for(j=0;j<i;j++)
            {
                printf("%d",n);
                n=n+2;
            }
        }
        printf("\n");
            
           
        }
        
    return 0;
}
