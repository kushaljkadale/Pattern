#include <stdio.h>

int main()
{
    int i,j,k,n;
    
    for(i=1;i<=5;i++)
    {
        for(j=0;j<i;j++)
        {
            if((j==1)||(j==2)||(j==3)||(j==4)||(j==5))
            printf("*");
            
            printf("%d",i);
        }
        printf("\n");
    }
    
    
    for(i=4;i>0;i--)
    {
       // n=i;
    
        for(j=0;j<n;j++)
        {
            if((j==1)||(j==2)||(j==3)||(j==4)||(j==5))
            printf("*");
            
            printf("%d",i);
        }
           // n--;
        printf("\n");
    }
    
    return 0;
}
